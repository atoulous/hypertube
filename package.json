{
  "name": "hypertube",
  "version": "1.0.0",
  "scripts": {
    "start-client": "cd client && npm run start",
    "start-server": "nodemon --exec babel-node --ignore client server",
    "start-db": "mongod --dbpath ./server/db",
    "start-dev": "concurrently --kill-others-on-fail 'npm run start-db' 'npm run start-server' 'npm run start-client'"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.2.2",
    "mongoose": "^5.1.4",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-42": "^1.2.4",
    "passport-github2": "^0.1.11",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "universal-cookie": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "nodemon": "^1.17.5"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
